<xaml:XamlApplication
    x:Class="MicaForEveryone.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:xaml="using:Microsoft.Toolkit.Win32.UI.XamlHost"
    xmlns:mux="using:Microsoft.UI.Xaml.Controls"
    xmlns:local="using:MicaForEveryone.UWP">
    <Application.Resources>
        <mux:XamlControlsResources>
            <mux:XamlControlsResources.MergedDictionaries>
                <ResourceDictionary>
                    <FontFamily x:Key="SymbolThemeFontFamily">Segoe Fluent Icons</FontFamily>
                </ResourceDictionary>
            </mux:XamlControlsResources.MergedDictionaries>
        </mux:XamlControlsResources>
    </Application.Resources>
</xaml:XamlApplication>
